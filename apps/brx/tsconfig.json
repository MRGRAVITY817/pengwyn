{
  "compilerOptions": {
		"baseUrl": "./src",
		"paths": {
			"@pages": ["./pages"],
			"@storage": ["./storage"],
			"@storage/*": ["./storage/*"],
		},
		"noEmit": false,
  },
  "extends": "tsconfig/react-library.json",
  "include": [ "src/**/*.ts", "src/**/*.tsx"],
  "exclude": ["node_modules"]
}
